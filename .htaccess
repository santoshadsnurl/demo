RewriteRule ^super-sub-categories/(.*)/(.*)/ super-sub-categories.php?alias=$1&pageno=$2 [L]
RewriteRule ^super-sub-categories/(.*)/(.*) super-sub-categories.php?alias=$1&pageno=$2 [L]

RewriteRule ^detail/(.*)/ detail.php?alias=$1 [L]
RewriteRule ^detail/(.*) detail.php?alias=$1 [L]

RewriteRule ^help/(.*)/ help.php?alias=$1 [L]
RewriteRule ^help/(.*) help.php?alias=$1 [L]

RewriteRule ^cartprogress/ cartprogress.php [L]
